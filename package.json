{
  "name": "md2googleslides",
  "version": "0.5.1",
  "description": "Convert Markdown to Google Slides",
  "engines" : { 
    "npm" : ">=8.19.0",
    "node" : ">=16.0.0 <20.0.0"
  },
  "type": "module",
  "exports": "index.js",
  "files": [
    "index.js",
    "bin/",
    "lib/",
    "LICENSE",
    "README.md",
    "CONTRIBUTING.md",
    "CHANGELOG.md"
  ],
  "scripts": {
    "clean": "gts clean",
    "compile": "tsc && babel --extensions '.ts,.js' --source-maps both -d lib/ src/",
    "prepublish": "npm run compile",
    "exec": "npm run compile && node bin/md2gslides.js",
    "test": "mocha --require ./test/register --timeout 5000 \"test/**/*.spec.ts\"",
    "test-debug": "mocha --debug-brk --inspect --require ./test/register --timeout 5000 \"test/**/*.spec.ts\"",
    "lint": "gts lint",
    "eslint-check": "eslint --print-config . | eslint-config-prettier-check",
    "fix": "gts fix",
    "prepare": "npm run compile",
    "pretest": "npm run compile",
    "posttest": "npm run lint"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com//googlesamples/md2googleslides"
  },
  "keywords": [
    "markdown",
    "google",
    "slides"
  ],
  "author": "Steven Bazyl",
  "contributors": "Emmanuel Schanzer",
  "license": "Apache-2.0",
  "dependencies": {
    "argparse": "^2.0.1",
    "canvas": "^2.11.2",
    "cli-progress": "^3.12.0",
    "debug": "^4.3.4",
    "file-url": "^4.0.0",
    "formdata-polyfill": "^4.0.10",
    "googleapis": "^130.0.0",
    "highlight.js": "^11.9.0",
    "inline-styles-parse": "^1.2.0",
    "layout": "^2.2.0",
    "lodash": "^4.17.21",
    "lowdb": "^1.0",
    "lowlight": "^2.9.0",
    "markdown-it": "^13.0.1",
    "markdown-it-attrs": "^4.1.4",
    "markdown-it-container": "^3.0.0",
    "markdown-it-emoji": "^2.0.2",
    "markdown-it-expand-tabs": "^1.0.13",
    "markdown-it-fence": "^0.1.3",
    "markdown-it-lazy-headers": "^0.1.3",
    "markdown-it-video": "^0.6.3",
    "mathjax-full": "^3.2.2",
    "mkdirp": "^3.0.1",
    "native-css": "^1.2.5",
    "opener": "^1.5.2",
    "parse-color": "^1.0.0",
    "probe-image-size": "^7.2.3",
    "promise-retry": "^2.0.1",
    "puppeteer": "^21.7.0",
    "sharp": "^0.32.1",
    "tmp": "0.2.1",
    "tmp-promise": "^3.0.3",
    "uuid": "^9.0.0"
  },
  "devDependencies": {
    "@babel/cli": "7.21.5",
    "@babel/core": "7.21.8",
    "@babel/plugin-proposal-class-properties": "7.18.6",
    "@babel/preset-env": "7.21.5",
    "@babel/preset-typescript": "7.21.5",
    "@babel/register": "7.21",
    "@types/chai": "^4.3.5",
    "@types/chai-as-promised": "^7.1.5",
    "@types/chai-subset": "^1.3.3",
    "@types/cli-progress": "^3.11.0",
    "@types/debug": "^4.1.7",
    "@types/jsonfile": "^6.1.0",
    "@types/lowdb": "1.0.11",
    "@types/lowlight": "^0.0.3",
    "@types/markdown-it": "12.2.3",
    "@types/mocha": "^10.0.1",
    "@types/mock-fs": "^4.13.1",
    "@types/node": "17.0.32",
    "@types/parse-color": "^1.0.1",
    "@types/parse5": "^6.0.3",
    "@types/probe-image-size": "^7.0.1",
    "@types/promise-retry": "^1.1.3",
    "@types/tmp": "^0.2.3",
    "@types/uuid": "^8.3.4",
    "chai": "4.3.7",
    "chai-as-promised": "7.1.1",
    "chai-subset": "1.6.0",
    "gts": "3.1.1",
    "mocha": "10.2.0",
    "mock-fs": "5.1.2",
    "nock": "13.2.4",
    "parse5": "^6.0.1",
    "typescript": "5.0.4"
  },
  "bin": {
    "md2gslides": "bin/md2gslides.js"
  }
}
